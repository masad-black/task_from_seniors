generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room{
  id String @id @default(uuid())
  name String
  chats String[] @default([])
}

model Chat {
  id String @id @default(uuid())
  message String
  username String

  createdAt DateTime @default(now())
}
