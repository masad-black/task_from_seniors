generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Phone {
  id String @id @default(uuid())
  name String
  battery Decimal
  brand Json?
  ram Int
  storgae Int
  process String
  images String[] @default([])
  bodySize Decimal
  camera Decimal
  quantity Int
  price Int
  discount Decimal?
  display String
  avaliableColors String[]
}
